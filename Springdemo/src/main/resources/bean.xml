<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
        http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">


    <bean id="user" class="com.example.springdemo.learn.zzzbean.User" scope="prototype" autowire="byType">
        <property name="name" value="jerry"/>
    </bean>

    <bean id="dog" class="com.example.springdemo.learn.zzzbean.Dog" scope="prototype" autowire="byType">
        <property name="name" value="dd"/>
    </bean>

    <bean id="cat" class="com.example.springdemo.learn.zzzbean.Cat" scope="prototype" autowire="byType">
        <property name="name" value="cc"/>
    </bean>


</beans>